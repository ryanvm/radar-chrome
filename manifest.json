{
  "manifest_version": 3,
  "name": "Weather Radar New Tab",
  "version": "1.0",
  "description": "Displays the current local weather radar on the new tab page.",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
